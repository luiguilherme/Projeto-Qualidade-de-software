                            <apex:column headerValue="Valor" value="{!f['value']}" styleClass="coluna"/>
                            <apex:column headerValue="Data" value="{!f['data']}" styleClass="coluna"/>
                        </apex:pageBlockTable>
                    </apex:pageBlock>
                </div>

