<apex:page renderAs="pdf" showHeader="false"
           applyHtmlTag="false" 
           standardStylesheets="false"
           controller="ExtractDetailedAsPdfController"
           action="{!executeStatement}">
    <html>
        <head>
            <style> 
                p {
                	text-align: center;
                }
                .tittle_block{
                 text-align: left;
                 font-weight: bold;
                }
                .container_body {
                  font-family: Arial, sans-serif;
                  font-size: 10px;
                  width: 99%;
                  display: block;
                  outline: dashed 1px black;
                }
                .container_350_200 {
                  font-family: Arial Unicode MS;
                  margin: 25px;
                  width: 350px;
                  height: 200px;
                  outline: dashed 1px black;
                }
                .container_width_100_height_100 {
                  font-family: Arial Unicode MS;
                  margin: 10px;
                  width: 100px;
                  height: 100px;
                }
                .wrapper {
                  display: grid;
                  grid-template-columns: 20% 20% 20%;
                  grid-gap: 10px;
                  color: #444;
                }
                .col1 {
                	width: 1% !important;
                }
                .col3 {
                	width: 45% !important;
                }
                .col4 {
                	text-align: right;
                }
                .col_second_session_1 {
                }
                .col_second_session_3 {
                	width: 46.8% !important;
                }
                .col_second_session_4 {
                	text-align: right;
                }
                .coluna {
                    margin-right: 20px;
                    flex-grow: 1;
                }
                .container {
                    display: flex;
                }
                tr.colHeader {
                    border: 3px solid #731d21;
                    border-radius: 10px;
                }
            
                td.colHeader {
                    background-color: #0070c0;
                    color: white;
                    padding: 10px;
            }
            </style>
        </head>
        <body class="container_body">
            <div>
                <p>DETALHAMENTO DE CONSUMO PARA SIMPLES CONFERÊNCIA - VIVO PRÉ-PAGO</p>
            </div>
            <apex:panelGrid columns="4" columnClasses="col1, col2, col3, col4" cellpadding="" width="100%">
                    <apex:panelGroup >
                        <apex:outputLabel ></apex:outputLabel>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <img src="{!$Resource.LogoVivo}" alt="Girl in a jacket" width="130" height="50"/>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <apex:outputLabel ></apex:outputLabel>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <apex:outputLabel > Telefonica Brasil S.A</apex:outputLabel><BR/>
                        <apex:outputLabel > Av. Engenheiro Luiz Carlos Berrini, 1376</apex:outputLabel><BR/>
                        <apex:outputLabel > 0457198 São Paulo SP</apex:outputLabel><BR/>
                        <apex:outputLabel > www.vivo.com.br</apex:outputLabel><BR/>
                        <apex:outputLabel > 108383949112 - 0255815700162</apex:outputLabel>
                    </apex:panelGroup>
            </apex:panelGrid>
            
            <apex:panelGrid columns="4" columnClasses="col_second_session_1, col_second_session_2, col_second_session_3,col_second_session_4" cellpadding="" width="100%">
                    <apex:panelGroup >
                        <apex:outputLabel >MASSA CONTROLE</apex:outputLabel><BR/>
                        <apex:outputLabel >BENTO, 100 CENTRO</apex:outputLabel><BR/>
                        <apex:outputLabel >01010-000 SÃO PAULO SP</apex:outputLabel>
                    </apex:panelGroup>
                    <apex:panelGroup >
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <apex:outputLabel ></apex:outputLabel>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <apex:outputLabel style="float: left;"> CELULAR:</apex:outputLabel> <apex:outputLabel value="{!lineNumber}"></apex:outputLabel><BR/>
                        <apex:outputLabel style="float: left;"> CONTA:</apex:outputLabel> <apex:outputLabel value="{!conta}"></apex:outputLabel><BR/>
                        <apex:outputLabel style="float: left;"> PLANO TARIFÁRIO:</apex:outputLabel> <apex:outputLabel value="{!plano}"></apex:outputLabel><BR/>
                        <apex:outputLabel style="float: left;"> EMISSÃO:</apex:outputLabel> <apex:outputLabel value="{!emissao}"></apex:outputLabel><BR/>
                        <apex:outputLabel style="float: left;"> PERÍODO:</apex:outputLabel> <apex:outputLabel value="{!periodo}"></apex:outputLabel><BR/>
                    </apex:panelGroup>
            </apex:panelGrid>

            <!--Todas as funções abaixo distribuem as  infromações que vem pela controller, exibindoas no pdf
                grande parte esta dividida por titulos <h3> segidos de <div> com o conjunto de informações referente ao titulo
                US: PTI1094-272
                Created by: Felipe Silveira
                Modified by: Dalvino Santos 
            -->
            <div class="block_font">
                <apex:outputText ><b>Plano Ativo Atual:</b></apex:outputText><apex:outputText value=" {!plano}" ></apex:outputText><BR/>   
            </div><br/>

            <div class="summary_info">
