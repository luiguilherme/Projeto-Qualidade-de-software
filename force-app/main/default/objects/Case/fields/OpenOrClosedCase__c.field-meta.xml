<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OpenOrClosedCase__c</fullName>
    <description>Campo utilizado para filtrar os valores de Status para Aberto ou Fechado.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status, &quot;Closed&quot;), &quot;Caso Fechado&quot;, &quot;Caso Aberto&quot;)</formula>
    <label>Caso Aberto ou Fechado</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
