<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>configuração personalizada responsável pelas configurações de expurgo exceto objetos de OM, pois serão expurgados pela Job Nativo do OM</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ObjectName__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Nome de API do objeto que vai ter os registros excluídos (“Account”, “Order”, “vlocity_cmt__OrchestrationItem__c” )</inlineHelpText>
        <label>Nome do Objeto</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PurgeEnabled__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Expurgo Habilitado</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>RetentionPeriodMeasurementUnit__c</fullName>
        <externalId>false</externalId>
        <label>Unidade de Medida do Período de Retenção</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RetentionPeriod__c</fullName>
        <description>Refine o período de retenção de registros do objeto selecionado. Do dia atual voltando no tempo nesse período, será mantido na org. Registros mais antigos serão deletados.</description>
        <externalId>false</externalId>
        <label>Período de Retenção</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Configurações de Expurgo</label>
    <visibility>Public</visibility>
</CustomObject>
