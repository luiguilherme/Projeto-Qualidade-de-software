<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContractGenerationId__c</fullName>
    <description>Campo fórmula que gera um Id numérico que utilizaremos para enviar para realizar a geração de contrato no GEDOC.</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(DATEVALUE(CreatedDate))) &amp; LPAD(TEXT(MONTH(DATEVALUE(CreatedDate))),2,'0') &amp; LPAD(TEXT(DAY(DATEVALUE(CreatedDate))),2,'0') &amp; LPAD(TEXT(HOUR(TIMEVALUE(CreatedDate))),2,'0') &amp; OrderNumber</formula>
    <label>Id de Geração do Contrato</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>