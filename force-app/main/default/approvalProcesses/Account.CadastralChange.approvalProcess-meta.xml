<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>RequestedFirstName__c</field>
        <field>RequestedLastName__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>UpdateAccountName</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>Cadastral Change</label>
        <name>Cadastral_Change</name>
    </approvalStep>
    <description>Approval Process utilizado no processo de aprovação via alçada.</description>
    <emailTemplate>unfiled$public/ApprovalProcessTemplateAlcada</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Reset_Approval_User_Name</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Approver_User</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Remarks</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Submitter_Comments</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Reset_Approval_User_Name</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Approver_User</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Remarks</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Requested_Last_Name_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Requested_Name_Reject</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Alteração Cadastral</label>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>Reset_Approval_User_Name</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Approver_User</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Remarks</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Requested_Last_Name</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Requested_Name</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
